mprs vpda [
    p.S <= q.S

    p.S coin! p.M.S
    p.M coin! p.M.M
    p.M tea! t
    p.M coffee! c

    t.M tea! t
    t.S coin! p.M.S

    c.M coffee! c
    c.S coin! p.M.S


    q.S coin? q.T.S
    q.S coin? q.C.S
    q.T coin? q.T.T
    q.C coin? q.C.C

    q.T tea! q
    q.T coffee? q

    q.C coffee! q
    q.C tea? q
]
